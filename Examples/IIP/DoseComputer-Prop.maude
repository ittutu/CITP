in ../../Tool/citp
in DoseComputer

fmod LOG-PROP is
    protecting LOG-READINGS .

    ceq max(D1:Dose, D2:Dose) < D:Dose = true if D1:Dose < D:Dose = true /\ D2:Dose < D:Dose = true
          [metadata "max"] .
    ceq relativeDose(R1:Reading, R2:Reading) < maxDose = true if (R1:Reading, R2:Reading, LR:ALR) : ALR
          [metadata "RelDose-ALR" nonexec] .
endfm

select #CITP# .
loop init .

(goal LOG-PROP |- ceq (dose(R:Reading, L:Log) < maxDose) = true if (R:Reading, readings L:Log) : ALR ;)
(tc ca imp)
(. init RelDose-ALR by
   R1:Reading <- R#1 ;
   R2:Reading <- reading RD#3 ;
   LR:ALR <- (reading RD'#4) , (readings L'#5) ;)
(. red)
