in ../../Tool/citp

in tas_explicit2.maude

select #CITP# .

loop init .

(goal TAS |-
ceq true = false if < open, (cs LS:LSet) > := R:Reach [nonexec];
ceq true = false if < S:Status, (cs cs LS:LSet) > := R:Reach [nonexec];)

(indx on R:Reach red)

eof

(cp-l eq(rs ~ cs)= false ; >< eq rs = cs ;)

(init 2 by )

(init 1 by )

(cp-l eq(open ~ close)= false ; >< eq close = open ;)

