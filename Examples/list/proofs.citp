goal REV is
  fm forall{L1:List, L2:List, L3:List}
     (L1:List @ L2:List) @ L3:List = L1:List @ (L2:List @ L3:List) .
|-{LISTS}
  fm forall {L1:List, L2:List, L3:List}
     rev(L1:List @ L2:List)= rev(L2:List)@ rev(L1:List) .
endg

begin proof PR of REV
  ind(L1:List) tc ca push-all red
qed

